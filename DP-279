#leetcode exercise 279 in DP: compute the least amount of square numbers you must add to obtain the number n

class Solution:
    def numSquares(self, n: int) -> int:
        dp=[0]*(n+1)
        dp[1]=1
        for j in range(2,n+1):
            #riempio dp[j]
            k=1
            dp[j]=dp[j-1]+1
            while k*k<=j:
                if dp[j-k*k]+1<dp[j]:
                    dp[j]=dp[j-k*k]+1
                k+=1
        return dp[n]
